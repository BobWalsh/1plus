<% if Jumpstart.config.stripe? %>
  <%= tag :meta,
          name: "stripe-key",
          content: Rails.application.credentials.development.dig(:stripe, :public_key) %>
  <%= javascript_include_tag 'https://js.stripe.com/v3/', 'data-turbolinks-track': 'reload' %>
  <%= javascript_pack_tag    'stripe', 'data-turbolinks-track': 'reload' %>
<% end %>
<% if Jumpstart.config.braintree? %>
  <%= javascript_include_tag 'https://js.braintreegateway.com/web/dropin/1.12.0/js/dropin.min.js', 'data-turbolinks-track': 'reload' %>
  <%= javascript_pack_tag    'braintree', 'data-turbolinks-track': 'reload' %>
<% end %>
